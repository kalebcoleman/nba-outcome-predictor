factor(preds_xgb_opt, levels = c(0,1)),
factor(test_feats$Target, levels = c(0,1))
)
cat(sprintf("XGBoost @ %.3f â†’ Accuracy: %.3f, Sensitivity: %.3f, Specificity: %.3f\n",
opt_xgb,
acc_opt,
cm_opt$byClass["Sensitivity"],
cm_opt$byClass["Specificity"]))
